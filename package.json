{
  "name": "care-calendar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:setup-db": "cd backend && npm run setup-db",
    "backend:seed-db": "cd backend && npm run seed-db",
    "backend:setup-history": "cd backend && npm run setup-history",
    "dev:all": "npm run dev-simple",
    "dev-simple": "node start-simple.js",
    "start": "node start-simple.js --production",
    "set-ip": "sed -i \"s/YOUR_IP_ADDRESS/$(hostname -I | awk '{print $1}')/\" public/index.html",
    "backend:reset": "cd backend && bash reset-db.sh"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bonjour": "^3.5.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.19",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "vite": "^5.2.10"
  }
}
